{
  "name": "iu-sport-bot",
  "type": "module",
  "version": "1.0.0-beta.1",
  "private": true,
  "packageManager": "pnpm@8.15.5",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest",
    "deps": "taze -wIr",
    "release": "bumpp -r package.json backend/package.json",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.11.6",
    "bumpp": "^9.4.0",
    "eslint": "^8.57.0",
    "eslint-plugin-format": "^0.1.0",
    "lint-staged": "^15.2.2",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.13.3",
    "tsx": "^4.7.1",
    "vitest": "^1.4.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
